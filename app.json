{
  "expo": {
    "name": "DMDC Dashboard App",
    "description": "App designed to monitor the power consumption statistics of a ESP32 based power meter",
    "slug": "github.com/DMDC-estagio/Dashboard-app", 
    "privacy": "public",
    "sdkVersion": "45.0.0",
    "platforms": [ "ios", "android" ],
    "version": "1.0.36",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [ "**/*" ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "gl.js.app",
      "infoPlist": {
      }
    },
    "android": {
      "package": "gl.js.app",
      "permissions": [ "CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL" ],
      "versionCode": 36
    }
  }
}